<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.editeur.org/onix/2.1/reference" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.editeur.org/onix/2.1/reference" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:attributeGroup name="releaseAttribute">
    <xs:annotation>
      <xs:documentation>Must be 3.0 for this release</xs:documentation>
    </xs:annotation>
    <xs:attribute name="release" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="3.0" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
</xs:schema>
